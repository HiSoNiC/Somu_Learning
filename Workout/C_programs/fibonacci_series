#include<stdio.h>

int main(){
    int num, f1 = 1, f2 = 1, fib = 0;
    printf("enter a positive number\n");
    scanf("%d", &num);
    //for first 3 term display o/p by if else condition
    if(num == 1)
        printf("Fibonacci sequence is : 0");
    else if(num == 2)
        printf("Fibonacci sequence is : 0 1");
    else if(num == 3)
        printf("Fibonacci sequence is : 0 1 1");
    else
        printf("Fibonacci sequence is : 0 1 1 ");
        for(int i = 4; i<=num; i++) {
            fib = f1 + f2;
            printf("%d ", fib);
            f2 = f1;
            f1 = fib;
        }
    printf("\n");
}
